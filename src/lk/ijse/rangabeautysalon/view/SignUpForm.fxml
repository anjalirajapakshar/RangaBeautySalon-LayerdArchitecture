<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="483.0" prefWidth="410.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.rangabeautysalon.controller.SignUpFormController">
   <children>
      <Label layoutX="51.0" layoutY="49.0" text="Sign-Up">
         <font>
            <Font name="Agency FB" size="24.0" />
         </font>
      </Label>
      <JFXButton buttonType="RAISED" layoutX="51.0" layoutY="370.0" onAction="#btnSignUpOnAction" prefHeight="36.0" prefWidth="304.0" style="-fx-background-radius: 50; -fx-background-color: gray;" text="Sign-Up">
         <font>
            <Font name="Agency FB" size="15.0" />
         </font>
      </JFXButton>
      <Label layoutX="142.0" layoutY="422.0" prefHeight="17.0" prefWidth="80.0" text="Back to Sign-In&gt;&gt;">
         <font>
            <Font name="Agency FB" size="14.0" />
         </font>
      </Label>
      <Label layoutX="222.0" layoutY="422.0" onMouseClicked="#lblSignInOnAction" text="Sign-In" textFill="#1928ee">
         <font>
            <Font name="Agency FB" size="14.0" />
         </font>
      </Label>
      <JFXTextField fx:id="txtUserName" layoutX="51.0" layoutY="126.0" onKeyPressed="#txtUseNameOnAction" prefHeight="29.0" prefWidth="304.0" promptText="Username" />
      <JFXTextField fx:id="txtRole" layoutX="51.0" layoutY="179.0" onKeyPressed="#txtRoleOnAction" prefHeight="29.0" prefWidth="304.0" promptText="Role" />
      <JFXTextField fx:id="txtPassword" layoutX="51.0" layoutY="233.0" onKeyPressed="#txtPasswordOnAction" prefHeight="29.0" prefWidth="304.0" promptText="Password" />
      <JFXTextField fx:id="txtConfirmPassword" layoutX="51.0" layoutY="292.0" onKeyPressed="#txtConfirmPasswordOnAction" prefHeight="29.0" prefWidth="304.0" promptText="Confirm Password" />
   </children>
</AnchorPane>
